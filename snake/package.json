{
  "name": "pixi-webpack-starter-kit",
  "version": "1.0.0",
  "description": "JS game dev quick-start",
  "scripts": {
    "private-build-dev": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "private-lint-root": "eslint *.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --content-base dist/",
    "build-dev": "npm-run-all private-lint-root private-build-dev",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "serve": "node server.js",
    "start": "npm-run-all build serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndyBarron/pixi-webpack-starter-kit"
  },
  "author": "Andy Barron <AndrewLBarron@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndyBarron/pixi-webpack-starter-kitissues"
  },
  "homepage": "https://github.com/AndyBarron/pixi-webpack-starter-kit#readme",
  "dependencies": {
    "axios": "0.15.3",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-polyfill": "6.23.0",
    "babel-preset-latest": "6.22.0",
    "clean-webpack-plugin": "0.1.15",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "eslint": "3.15.0",
    "eslint-loader": "1.6.1",
    "express": "4.14.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "generate-asset-webpack-plugin": "0.3.0",
    "howler": "2.0.2",
    "html-webpack-plugin": "2.28.0",
    "javascript-natural-sort": "0.7.1",
    "npm-run-all": "4.0.1",
    "pixi.js": "4.3.5",
    "raf": "3.3.0",
    "slash": "1.0.0",
    "style-loader": "0.13.1",
    "texture-atlas": "0.2.1",
    "walk": "2.3.9",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "engines": {
    "node": "7.5.0"
  }
}
